                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select att.NOBR,base.NAME_C,dept.D_NAME,att.ADATE,r.ROTENAME from attend att left join base  on att.NOBR=base.NOBR left join ROTE r on att.ROTE=r.ROTE left join BASETTS on att.NOBR = basetts.NOBR and att.ADATE between BASETTS.ADATE and BASETTS.DDATE left join DEPT on BASETTS.DEPT=DEPT.D_NO where att.ROTE &lt;&gt; '00' and att.NOBR not in (select NOBR from CARD where NOBR=att.NOBR and ADATE=att.ADATE) and att.ADATE &lt; dateadd(day,-1,getdate())  order by att.adate desc,att.nobr</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
