<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<appSettings>
		<add key="aspnet:UseTaskFriendlySynchronizationContext" value="true" />
		<add key="JBModule.Data.HrDBConnectionString" value="JBHR.Properties.Settings.JBHRConnectionString" />
		<add key="JBHR.DLL.ConnectionString" value="JBHR.Properties.Settings.JBHRConnectionString" />
		<add key="Message.LogPath" value="C:\Temp" />
	</appSettings>
	<connectionStrings>
		<add name="JBHR.Properties.Settings.JBHRConnectionString" connectionString="Data Source=192.168.1.12;Initial Catalog=EMCCN;Persist Security Info=True;User ID=jb;Password=^Hsx9bu5t@" providerName="System.Data.SqlClient" />
	</connectionStrings>
  <system.web>
    <compilation debug="true" targetFramework="4.0" />
  </system.web>
  <system.serviceModel>
    <behaviors>
      <serviceBehaviors>
        <behavior>
          <!-- 為避免洩露中繼資料資訊，部署前請將下面的值設為 false -->
          <serviceMetadata httpGetEnabled="true"/>
          <!-- 為接收錯誤中的例外狀況詳細資料以用於偵錯用途，請將下面的值設定為 true。請在部署之前將該值設定為 false，以避免例外狀況資訊洩漏 -->
          <serviceDebug includeExceptionDetailInFaults="false"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <serviceHostingEnvironment multipleSiteBindingsEnabled="true" />
  </system.serviceModel>
 <system.webServer>
    <modules runAllManagedModulesForAllRequests="true"/>
    <!--
        若要在偵錯期間瀏覽 Web 應用程式根目錄，請將下面的值設為 true。
        請在部署之前將該值設定為 false，以避免 Web 應用程式資料夾資訊洩漏。
      -->
    <directoryBrowse enabled="true"/>
  </system.webServer>

</configuration>
