                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT          BASE.NOBR, BASE.NAME_C, DEPT.D_NAME, ROTECHG.ADATE, ROTE.ROTENAME, ROTECHG.CODE,ROTECHG.KEY_DATE
FROM              ROTECHG INNER JOIN
                            BASE ON ROTECHG.NOBR = BASE.NOBR INNER JOIN
                            BASETTS ON ROTECHG.NOBR = BASETTS.NOBR LEFT OUTER JOIN
                            DEPT ON BASETTS.DEPT = DEPT.D_NO LEFT OUTER JOIN
                            ROTE ON ROTECHG.ROTE = ROTE.ROTE
WHERE          (ROTECHG.CODE &lt;&gt; '') AND (ROTECHG.ROTE &lt;&gt; '00') AND (ROTECHG.ADATE BETWEEN BASETTS.ADATE AND 
                            BASETTS.DDATE)
ORDER BY   ROTECHG.ADATE DESC</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
            </MainSource>
